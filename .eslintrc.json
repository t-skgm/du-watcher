{
  "plugins": ["neverthrow"],
  "extends": ["plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "neverthrow/must-use-result": "error"
  }
}
