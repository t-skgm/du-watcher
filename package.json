{
  "name": "du-watcher",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "prisma generate && next dev",
    "build": "prisma generate && next build",
    "script": "prisma generate && node -r ts-node/register -r tsconfig-paths/register",
    "db:migrate": "prisma db push",
    "start": "next start",
    "start:watcher": "pnpm script ./scripts/watcher.ts",
    "lint": "next lint",
    "test": "vitest",
    "typecheck": "tsc -p . --noEmit",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.0.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-form": "^0.0.3",
    "@tailwindcss/forms": "^0.5.4",
    "autoprefixer": "10.4.14",
    "immer": "^10.0.2",
    "next": "13.4.12",
    "node-html-parser": "^6.1.5",
    "postcss": "8.4.27",
    "prisma": "5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "use-immer": "^0.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "esbuild": "^0.18.17",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^1.10.12",
    "typescript": "5.1.6",
    "vitest": "^0.33.0"
  }
}
